    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	СгруппированныеДанные.Дата КАК Дата,
	СгруппированныеДанные.Номер КАК Номер,
	СгруппированныеДанные.Документ КАК Документ,
	СгруппированныеДанные.Организация КАК Организация,
	ВЫБОР
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 5
			ТОГДА СгруппированныеДанные.ДатаОтраженияНеУказаныСчетаУчета
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 4
			ТОГДА СгруппированныеДанные.ДатаОтраженияКОтражениюВУчетеВручную
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 3
			ТОГДА СгруппированныеДанные.ДатаОтраженияОтраженоВУчетеВручную
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 2
			ТОГДА СгруппированныеДанные.ДатаОтраженияКОтражениюВРеглУчете
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 1
			ТОГДА СгруппированныеДанные.ДатаОтраженияОтраженоВРеглУчете
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 0 
			ТОГДА ДАТАВРЕМЯ(1,1,1)
	КОНЕЦ КАК ДатаОтражения,
	
	ВЫБОР
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 5
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.НеУказаныСчетаУчета)
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 4
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.КОтражениюВУчетеВручную)
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 3
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.ОтраженоВУчетеВручную)
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 2
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.КОтражениюВРеглУчете)
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 1
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.ОтраженоВРеглУчете)
		КОГДА СгруппированныеДанные.СводныйСтатусОтраженияЧислом = 0 
			ТОГДА ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.ПустаяСсылка)
	КОНЕЦ КАК СтатусОтражения,
	
	СгруппированныеДанные.СтатусПроверки КАК СтатусПроверки,
	СгруппированныеДанные.АвторПроверкиДокумента КАК АвторПроверкиДокумента,
	СгруппированныеДанные.АвторИзмененияДокумента КАК АвторИзмененияДокумента,
	СгруппированныеДанные.ДатаПроверкиДокумента КАК ДатаПроверкиДокумента,
	СгруппированныеДанные.ДатаИзмененияДокумента КАК ДатаИзмененияДокумента,
	СгруппированныеДанные.ТипДокумента КАК ТипДокумента,
	ВЫРАЗИТЬ(&amp;Автор КАК Справочник.Пользователи) КАК Автор,
	ВЫРАЗИТЬ(&amp;Договор КАК Справочник.ДоговорыКонтрагентов) КАК Договор,
	ВЫРАЗИТЬ(&amp;Касса КАК Справочник.Кассы) КАК Касса,
	ВЫРАЗИТЬ(&amp;Контрагент КАК Справочник.Контрагенты) КАК Контрагент,
	ВЫРАЗИТЬ(&amp;Партнер КАК Справочник.Партнеры) КАК Партнер,
	ВЫРАЗИТЬ(&amp;Склад КАК Справочник.Склады) КАК Склад,
	ВЫРАЗИТЬ(&amp;ХозяйственнаяОперация КАК Перечисление.ХозяйственныеОперации) КАК ХозяйственнаяОперация,
	ВЫРАЗИТЬ(&amp;СуммаДокумента КАК ЧИСЛО(15, 3)) КАК СуммаДокумента,
	ВЫРАЗИТЬ(&amp;Валюта КАК Справочник.Валюты) КАК Валюта
{ВЫБРАТЬ
	Дата,
	Номер,
	Документ.*,
	Организация.*,
	СтатусОтражения,
	СтатусПроверки,
	АвторПроверкиДокумента.*,
	АвторИзмененияДокумента.*,
	ДатаПроверкиДокумента,
	ДатаИзмененияДокумента,
	ТипДокумента}
ИЗ
	(ВЫБРАТЬ
		МАКСИМУМ(ОбъединенныеДанные.Дата) КАК Дата,
		МАКСИМУМ(ОбъединенныеДанные.Номер) КАК Номер,
		ОбъединенныеДанные.Документ КАК Документ,
		ОбъединенныеДанные.Организация КАК Организация,
		
		МАКСИМУМ(ОбъединенныеДанные.ДатаОтраженияНеУказаныСчетаУчета)     КАК ДатаОтраженияНеУказаныСчетаУчета,
		МАКСИМУМ(ОбъединенныеДанные.ДатаОтраженияКОтражениюВУчетеВручную) КАК ДатаОтраженияКОтражениюВУчетеВручную,
		МАКСИМУМ(ОбъединенныеДанные.ДатаОтраженияОтраженоВУчетеВручную)   КАК ДатаОтраженияОтраженоВУчетеВручную,
		МАКСИМУМ(ОбъединенныеДанные.ДатаОтраженияКОтражениюВРеглУчете)    КАК ДатаОтраженияКОтражениюВРеглУчете,
		МАКСИМУМ(ОбъединенныеДанные.ДатаОтраженияОтраженоВРеглУчете)      КАК ДатаОтраженияОтраженоВРеглУчете,
		
		МАКСИМУМ(СтатусОтраженияЧислом) КАК СводныйСтатусОтраженияЧислом,
		
		МАКСИМУМ(ОбъединенныеДанные.СтатусПроверки) КАК СтатусПроверки,
		МАКСИМУМ(ОбъединенныеДанные.АвторПроверкиДокумента) КАК АвторПроверкиДокумента,
		МАКСИМУМ(ОбъединенныеДанные.АвторИзмененияДокумента) КАК АвторИзмененияДокумента,
		МАКСИМУМ(ОбъединенныеДанные.ДатаПроверкиДокумента) КАК ДатаПроверкиДокумента,
		МАКСИМУМ(ОбъединенныеДанные.ДатаИзмененияДокумента) КАК ДатаИзмененияДокумента,
		ОбъединенныеДанные.ТипДокумента КАК ТипДокумента
	ИЗ
		(ВЫБРАТЬ РАЗЛИЧНЫЕ
			ЕСТЬNULL(ДанныеПервичныхДокументов.ДатаРегистратора, ОтражениеДокументовВРеглУчете.Период) КАК Дата,
			ЕСТЬNULL(ДанныеПервичныхДокументов.НомерРегистратора, &quot;&quot;) КАК Номер,
			ОтражениеДокументовВРеглУчете.Регистратор КАК Документ,
			ОтражениеДокументовВРеглУчете.Организация КАК Организация,
			
			ВЫБОР 
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.НеУказаныСчетаУчета)
					ТОГДА ОтражениеДокументовВРеглУчете.ДатаОтражения
				ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
			КОНЕЦ КАК ДатаОтраженияНеУказаныСчетаУчета,
			ВЫБОР 
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.КОтражениюВУчетеВручную)
					ТОГДА ОтражениеДокументовВРеглУчете.ДатаОтражения
				ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
			КОНЕЦ КАК ДатаОтраженияКОтражениюВУчетеВручную,
			ВЫБОР 
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.ОтраженоВУчетеВручную)
					ТОГДА ОтражениеДокументовВРеглУчете.ДатаОтражения
				ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
			КОНЕЦ КАК ДатаОтраженияОтраженоВУчетеВручную,
			ВЫБОР 
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.КОтражениюВРеглУчете)
					ТОГДА ОтражениеДокументовВРеглУчете.ДатаОтражения
				ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
			КОНЕЦ КАК ДатаОтраженияКОтражениюВРеглУчете,
			ВЫБОР 
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.ОтраженоВРеглУчете)
					ТОГДА ОтражениеДокументовВРеглУчете.ДатаОтражения
				ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
			КОНЕЦ КАК ДатаОтраженияОтраженоВРеглУчете,
			
			ВЫБОР
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.НеУказаныСчетаУчета)
					ТОГДА 5
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.КОтражениюВУчетеВручную)
					ТОГДА 4
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.ОтраженоВУчетеВручную)
					ТОГДА 3
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.КОтражениюВРеглУчете)
					ТОГДА 2
				КОГДА ОтражениеДокументовВРеглУчете.Статус = ЗНАЧЕНИЕ(Перечисление.СтатусыОтраженияДокументовВРеглУчете.ОтраженоВРеглУчете)
					ТОГДА 1
			КОНЕЦ КАК СтатусОтраженияЧислом,
				
			ВЫБОР
				КОГДА ТИПЗНАЧЕНИЯ(ОтражениеДокументовВРеглУчете.Регистратор) В (&amp;СписокПроверяемыхТипов)
					ТОГДА ЗНАЧЕНИЕ(Перечисление.ЭтапыПроверкиДокументаВРеглУчете.НеПроверен)
				ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.ЭтапыПроверкиДокументаВРеглУчете.ПустаяСсылка)
			КОНЕЦ КАК СтатусПроверки,
			ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка) КАК АвторПроверкиДокумента,
			ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка) КАК АвторИзмененияДокумента,
			ДАТАВРЕМЯ(1, 1, 1) КАК ДатаПроверкиДокумента,
			ДАТАВРЕМЯ(1, 1, 1) КАК ДатаИзмененияДокумента,
			ТИПЗНАЧЕНИЯ(ОтражениеДокументовВРеглУчете.Регистратор) КАК ТипДокумента
		ИЗ
			РегистрСведений.ОтражениеДокументовВРеглУчете КАК ОтражениеДокументовВРеглУчете
			ЛЕВОЕ СОЕДИНЕНИЕ
				РегистрСведений.ДанныеПервичныхДокументов КАК ДанныеПервичныхДокументов
			ПО
				ОтражениеДокументовВРеглУчете.Организация = ДанныеПервичныхДокументов.Организация
				И ОтражениеДокументовВРеглУчете.Регистратор = ДанныеПервичныхДокументов.Документ

		{ГДЕ
			ОтражениеДокументовВРеглУчете.ДатаОтражения >= &amp;НачалоПериода,
			ОтражениеДокументовВРеглУчете.ДатаОтражения &lt;= &amp;КонецПериода,
			ОтражениеДокументовВРеглУчете.Статус КАК СтатусОтражения,
			ОтражениеДокументовВРеглУчете.Регистратор.* КАК Документ,
			ОтражениеДокументовВРеглУчете.Организация.*,
			(ТИПЗНАЧЕНИЯ(ОтражениеДокументовВРеглУчете.Регистратор)) КАК ТипДокумента}
		
		ОБЪЕДИНИТЬ ВСЕ
		
		ВЫБРАТЬ РАЗЛИЧНЫЕ
			ДАТАВРЕМЯ(1, 1, 1),
			&quot;&quot;,
			СтатусыПроверкиДокументов.Документ,
			СтатусыПроверкиДокументов.Организация,
			ДАТАВРЕМЯ(1, 1, 1),
			ДАТАВРЕМЯ(1, 1, 1),
			ДАТАВРЕМЯ(1, 1, 1),
			ДАТАВРЕМЯ(1, 1, 1),
			ДАТАВРЕМЯ(1, 1, 1),
			0,
			ВЫБОР
				КОГДА &amp;ЕстьБлокировка
					ТОГДА ВЫБОР
							КОГДА СтатусыПроверкиДокументов.СтатусПроверки = ЗНАЧЕНИЕ(Перечисление.ЭтапыПроверкиДокументаВРеглУчете.КПовторнойПроверке)
								ТОГДА ЗНАЧЕНИЕ(Перечисление.ЭтапыПроверкиДокументаВРеглУчете.НеПроверен)
							ИНАЧЕ СтатусыПроверкиДокументов.СтатусПроверки
						КОНЕЦ
				ИНАЧЕ СтатусыПроверкиДокументов.СтатусПроверки
			КОНЕЦ,
			СтатусыПроверкиДокументов.Проверил,
			СтатусыПроверкиДокументов.Изменил,
			СтатусыПроверкиДокументов.ДатаПроверки,
			СтатусыПроверкиДокументов.ДатаИзменения,
			ТИПЗНАЧЕНИЯ(СтатусыПроверкиДокументов.Документ)
		ИЗ
			РегистрСведений.СтатусыПроверкиДокументов КАК СтатусыПроверкиДокументов
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ
				РегистрСведений.ОтражениеДокументовВРеглУчете КАК ОтражениеДокументовВРеглУчете
			ПО
				ОтражениеДокументовВРеглУчете.Регистратор = СтатусыПроверкиДокументов.Документ
				И ОтражениеДокументовВРеглУчете.Организация = СтатусыПроверкиДокументов.Организация
		{ГДЕ
			ОтражениеДокументовВРеглУчете.ДатаОтражения >= &amp;НачалоПериода,
			ОтражениеДокументовВРеглУчете.ДатаОтражения &lt;= &amp;КонецПериода,
			ОтражениеДокументовВРеглУчете.Статус КАК СтатусОтражения,
			ОтражениеДокументовВРеглУчете.Регистратор.* КАК Документ,
			ОтражениеДокументовВРеглУчете.Организация.*,
			(ТИПЗНАЧЕНИЯ(ОтражениеДокументовВРеглУчете.Регистратор)) КАК ТипДокумента}
		) КАК ОбъединенныеДанные
	
	СГРУППИРОВАТЬ ПО
		ОбъединенныеДанные.Документ,
		ОбъединенныеДанные.ТипДокумента,
		ОбъединенныеДанные.Организация) КАК СгруппированныеДанные
		//#Таблица
{ГДЕ
