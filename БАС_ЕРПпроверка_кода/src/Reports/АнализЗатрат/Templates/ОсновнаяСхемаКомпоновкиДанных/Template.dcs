				<dataPath>ДокументПоступленияРасходов</dataPath>
				<field>ДокументПоступленияРасходов</field>
				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>ru</v8:lang>
						<v8:content>Документ поступления расходов</v8:content>
					</v8:item>
					<v8:item>
						<v8:lang>uk</v8:lang>
						<v8:content>Документ надходження витрат</v8:content>
					</v8:item>
				</title>
			</field>
			<field xsi:type="DataSetFieldField">
				<dataPath>АналитикаУчетаПродукции</dataPath>
				<field>АналитикаУчетаПродукции</field>
				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>ru</v8:lang>
						<v8:content>Аналитика учета продукции</v8:content>
					</v8:item>
					<v8:item>
						<v8:lang>uk</v8:lang>
						<v8:content>Аналітика обліку продукції</v8:content>
					</v8:item>
				</title>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>// !!! ИСПОЛЬЗУЮТСЯ ТЕГИ !!!

ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	Сегменты.Номенклатура КАК Номенклатура,
	Сегменты.Характеристика КАК Характеристика,
	ИСТИНА КАК ИспользуетсяОтборПоСегментуНоменклатуры
ПОМЕСТИТЬ ОтборПоСегментуНоменклатуры
ИЗ
	РегистрСведений.НоменклатураСегмента КАК Сегменты
{ГДЕ
	Сегменты.Сегмент.* КАК СегментНоменклатуры,
	Сегменты.Номенклатура.* КАК Номенклатура,
	Сегменты.Характеристика.* КАК Характеристика}

ИНДЕКСИРОВАТЬ ПО
	Номенклатура,
	Характеристика,
	ИспользуетсяОтборПоСегментуНоменклатуры
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Затраты.Организация                                            КАК Организация,
	0                                                              КАК ВидДвижения,
	Затраты.АналитикаУчетаНоменклатуры                             КАК АналитикаУчетаНоменклатуры,
	Затраты.ВидЗапасов                                             КАК ВидЗапасов,
	Затраты.ДокументПоступления                                    КАК ДокументПоступления,
	Затраты.СтатьяКалькуляции                                      КАК СтатьяКалькуляции,
	НЕОПРЕДЕЛЕНО                                                   КАК Регистратор,
	ЕСТЬNULL(РасчетСебестоимости.Ссылка.МетодОценки, НЕОПРЕДЕЛЕНО) КАК МетодОценки
ПОМЕСТИТЬ Движения
ИЗ
	РегистрНакопления.ПартииНезавершенногоПроизводства.Остатки({(&amp;НачалоПериода)}, ) КАК Затраты
		
	ЛЕВОЕ СОЕДИНЕНИЕ Документ.РасчетСебестоимостиТоваров.Организации КАК РасчетСебестоимости
	ПО (&amp;НачалоПериода = НАЧАЛОПЕРИОДА(РасчетСебестоимости.Ссылка.Дата, МЕСЯЦ))
		И Затраты.Организация = РасчетСебестоимости.Организация
		И (РасчетСебестоимости.Ссылка.Проведен)
		И (РасчетСебестоимости.Ссылка.МетодОценки = ЗНАЧЕНИЕ(Перечисление.МетодыОценкиСтоимостиТоваров.ФИФОСкользящаяОценка))
{ГДЕ
	((Затраты.АналитикаУчетаНоменклатуры.Номенклатура, Затраты.АналитикаУчетаНоменклатуры.Характеристика) В
		(ВЫБРАТЬ
			ОтборПоСегментуНоменклатуры.Номенклатура,
			ОтборПоСегментуНоменклатуры.Характеристика
		ИЗ
			ОтборПоСегментуНоменклатуры
		ГДЕ
			ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)
	)
}

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Затраты.Организация                                            КАК Организация,
	ВЫБОР КОГДА Затраты.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
		ТОГДА 1
		ИНАЧЕ -1
	КОНЕЦ                                                          КАК ВидДвижения,
	Затраты.АналитикаУчетаНоменклатуры                             КАК АналитикаУчетаНоменклатуры,
	Затраты.ВидЗапасов                                             КАК ВидЗапасов,
	Затраты.ДокументПоступления                                    КАК ДокументПоступления,
	Затраты.СтатьяКалькуляции                                      КАК СтатьяКалькуляции,
	Затраты.Регистратор                                            КАК Регистратор,
	ЕСТЬNULL(РасчетСебестоимости.Ссылка.МетодОценки, НЕОПРЕДЕЛЕНО) КАК МетодОценки
ИЗ
	РегистрНакопления.ПартииНезавершенногоПроизводства КАК Затраты
		
	ЛЕВОЕ СОЕДИНЕНИЕ Документ.РасчетСебестоимостиТоваров.Организации КАК РасчетСебестоимости
	ПО (&amp;НачалоПериода = НАЧАЛОПЕРИОДА(РасчетСебестоимости.Ссылка.Дата, МЕСЯЦ))
		И Затраты.Организация = РасчетСебестоимости.Организация
		И (РасчетСебестоимости.Ссылка.Проведен)
		И (РасчетСебестоимости.Ссылка.МетодОценки = ЗНАЧЕНИЕ(Перечисление.МетодыОценкиСтоимостиТоваров.ФИФОСкользящаяОценка))
ГДЕ
	Затраты.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
{ГДЕ
	((Затраты.АналитикаУчетаНоменклатуры.Номенклатура, Затраты.АналитикаУчетаНоменклатуры.Характеристика) В
			(ВЫБРАТЬ
				ОтборПоСегментуНоменклатуры.Номенклатура,
				ОтборПоСегментуНоменклатуры.Характеристика
			ИЗ
				ОтборПоСегментуНоменклатуры
			ГДЕ
				ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)
	)
}

СГРУППИРОВАТЬ ПО
	Затраты.Организация,
	Затраты.ВидДвижения,
	Затраты.АналитикаУчетаНоменклатуры,
	Затраты.ВидЗапасов,
	Затраты.ДокументПоступления,
	Затраты.СтатьяКалькуляции,
	Затраты.Регистратор,
	ЕСТЬNULL(РасчетСебестоимости.Ссылка.МетодОценки, НЕОПРЕДЕЛЕНО)
;
////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Движения.Регистратор                                             КАК Регистратор,
	Движения.ВидДвижения                                             КАК ВидДвижения,
	Движения.Организация                                             КАК Организация,
	Движения.АналитикаУчетаНоменклатуры                              КАК АналитикаУчетаНоменклатуры,
	Движения.ВидЗапасов                                              КАК ВидЗапасов,
	СУММА(Затраты.КоличествоОстаток)                                 КАК Количество,
	СУММА(Затраты.КоличествоОстаток * Стоимости.Стоимость)                  КАК Сумма,
	СУММА(Затраты.КоличествоОстаток * Стоимости.СтоимостьДопРасходы)        КАК СуммаДопРасходов,
	СУММА(Затраты.КоличествоОстаток * Стоимости.СтоимостьБезНДС)            КАК СуммаБезНДС,
	СУММА(Затраты.КоличествоОстаток * Стоимости.СтоимостьДопРасходыБезНДС)  КАК СуммаДопРасходовБезНДС,
	СУММА(Затраты.КоличествоОстаток * Стоимости.СтоимостьРеглБезНДС +             
		Затраты.КоличествоОстаток * (ВЫБОР КОГДА Затраты.АналитикаУчетаПартий.НалоговоеНазначение.ВидДеятельностиНДС = ЗНАЧЕНИЕ(Перечисление.ВидыДеятельностиНДС.Необлагаемая)
		ТОГДА
			Стоимости.НДСРегл
		ИНАЧЕ
			0
		КОНЕЦ)
	)															            КАК СуммаРегл,
	Движения.МетодОценки                                             КАК МетодОценки,
	Движения.СтатьяКалькуляции                                       КАК СтатьяКалькуляции
ПОМЕСТИТЬ ДвиженияССуммами
ИЗ
	РегистрНакопления.ПартииНезавершенногоПроизводства.Остатки({(&amp;НачалоПериода)}, ) КАК Затраты
		
	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Движения КАК Движения
	ПО (Движения.Организация = Затраты.Организация)
		И (Движения.АналитикаУчетаНоменклатуры = Затраты.АналитикаУчетаНоменклатуры)
		И (Движения.ВидЗапасов = Затраты.ВидЗапасов)
		И (Движения.ДокументПоступления = Затраты.ДокументПоступления)
		И (Движения.ВидДвижения = 0)
	
	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Константы КАК Константы
	ПО (ИСТИНА)
	
	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СтоимостьТоваров.СрезПоследних({(&amp;ПредыдущийМесяц)}, ) КАК Стоимости
	ПО (Стоимости.Организация = Затраты.Организация)
		И (Стоимости.АналитикаУчетаНоменклатуры = Затраты.АналитикаУчетаНоменклатуры)
		И (Стоимости.ВидЗапасов = Затраты.ВидЗапасов
			ИЛИ НЕ Константы.УчитыватьСебестоимостьТоваровПоВидамЗапасов)
		И (Стоимости.РазделУчета = ЗНАЧЕНИЕ(Перечисление.РазделыУчетаСебестоимостиТоваров.ПроизводственныеЗатраты))
ГДЕ
	Движения.МетодОценки = НЕОПРЕДЕЛЕНО
	И Движения.Регистратор = НЕОПРЕДЕЛЕНО

СГРУППИРОВАТЬ ПО
	Движения.Регистратор,
	Движения.ВидДвижения,
	Движения.Организация,
	Движения.АналитикаУчетаНоменклатуры,
	Движения.ВидЗапасов,
	Движения.МетодОценки,
	Движения.СтатьяКалькуляции

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Движения.Регистратор,
	Движения.ВидДвижения,
	Движения.Организация,
	Движения.АналитикаУчетаНоменклатуры,
	Движения.ВидЗапасов,
	СУММА(Затраты.Количество),
	СУММА(Затраты.Количество * Стоимости.Стоимость),
	СУММА(Затраты.Количество * Стоимости.СтоимостьДопРасходы),
	СУММА(Затраты.Количество * Стоимости.СтоимостьБезНДС),
	СУММА(Затраты.Количество * Стоимости.СтоимостьДопРасходыБезНДС),
	СУММА(Затраты.Количество * (Стоимости.СтоимостьРеглБезНДС +             
		ВЫБОР 
		КОГДА Затраты.АналитикаУчетаПартий.НалоговоеНазначение.ВидДеятельностиНДС = ЗНАЧЕНИЕ(Перечисление.ВидыДеятельностиНДС.Необлагаемая)
			ТОГДА Стоимости.НДСРегл
		ИНАЧЕ
			0
		КОНЕЦ)
	) КАК СуммаРегл,
	Движения.МетодОценки,
	Движения.СтатьяКалькуляции
ИЗ
	РегистрНакопления.ПартииНезавершенногоПроизводства КАК Затраты
		
	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Движения КАК Движения
	ПО (Движения.Организация = Затраты.Организация)
		И (Движения.Регистратор = Затраты.Регистратор)
		И (Движения.АналитикаУчетаНоменклатуры = Затраты.АналитикаУчетаНоменклатуры)
		И (Движения.ВидЗапасов = Затраты.ВидЗапасов)
		И (Движения.ДокументПоступления = Затраты.ДокументПоступления)
		И Движения.ВидДвижения = ВЫБОР
			КОГДА Затраты.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
				ТОГДА 1
				ИНАЧЕ -1
			КОНЕЦ
	
	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Константы КАК Константы
	ПО (ИСТИНА)
	
	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СтоимостьТоваров КАК Стоимости
	ПО (Стоимости.Организация = Затраты.Организация)
		И Стоимости.Период = НАЧАЛОПЕРИОДА(Затраты.Период, МЕСЯЦ)
		И (Стоимости.АналитикаУчетаНоменклатуры = Затраты.АналитикаУчетаНоменклатуры)
		И (Стоимости.ВидЗапасов = Затраты.ВидЗапасов
			ИЛИ НЕ Константы.УчитыватьСебестоимостьТоваровПоВидамЗапасов)
		И (Стоимости.РазделУчета = ЗНАЧЕНИЕ(Перечисление.РазделыУчетаСебестоимостиТоваров.ПроизводственныеЗатраты))
ГДЕ
	Движения.МетодОценки = НЕОПРЕДЕЛЕНО
	И Движения.Регистратор &lt;&gt; НЕОПРЕДЕЛЕНО

СГРУППИРОВАТЬ ПО
	Движения.Регистратор,
	Движения.ВидДвижения,
	Движения.Организация,
	Движения.АналитикаУчетаНоменклатуры,
	Движения.ВидЗапасов,
	Движения.МетодОценки,
	Движения.СтатьяКалькуляции

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Движения.Регистратор,
	Движения.ВидДвижения,
	Движения.Организация,
	Движения.АналитикаУчетаНоменклатуры,
	Движения.ВидЗапасов,
	СУММА(Затраты.КоличествоОстаток),
	СУММА(Затраты.СтоимостьОстаток),
	0,
	СУММА(Затраты.СтоимостьБезНДСОстаток),
	0,
	СУММА(Затраты.СтоимостьРеглОстаток + 
		ВЫБОР КОГДА Затраты.АналитикаУчетаПартий.НалоговоеНазначение.ВидДеятельностиНДС = ЗНАЧЕНИЕ(Перечисление.ВидыДеятельностиНДС.Необлагаемая)
		ТОГДА
			Затраты.НДСРеглОстаток
		ИНАЧЕ
			0
		КОНЕЦ
	) КАК СуммаРегл,
	Движения.МетодОценки,
	Движения.СтатьяКалькуляции
ИЗ
	РегистрНакопления.ПартииНезавершенногоПроизводства.Остатки({(&amp;НачалоПериода)}, ) КАК Затраты
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Движения КАК Движения
		ПО (Движения.Организация = Затраты.Организация)
			И (Движения.АналитикаУчетаНоменклатуры = Затраты.АналитикаУчетаНоменклатуры)
			И (Движения.ВидЗапасов = Затраты.ВидЗапасов)
			И (Движения.ДокументПоступления = Затраты.ДокументПоступления)
			И (Движения.ВидДвижения = 0)
ГДЕ
	Движения.МетодОценки &lt;&gt; НЕОПРЕДЕЛЕНО
	И Движения.Регистратор = НЕОПРЕДЕЛЕНО

СГРУППИРОВАТЬ ПО
	Движения.Регистратор,
	Движения.ВидДвижения,
	Движения.Организация,
	Движения.АналитикаУчетаНоменклатуры,
	Движения.ВидЗапасов,
	Движения.МетодОценки,
	Движения.СтатьяКалькуляции

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Движения.Регистратор,
	Движения.ВидДвижения,
	Движения.Организация,
	Движения.АналитикаУчетаНоменклатуры,
	Движения.ВидЗапасов,
	0,
	СУММА(Затраты.СтоимостьОстаток),
	0,
	СУММА(Затраты.СтоимостьБезНДСОстаток),
	0,
	СУММА(Затраты.СтоимостьРеглОстаток +             
		ВЫБОР 
		КОГДА Затраты.АналитикаУчетаПартий.НалоговоеНазначение.ВидДеятельностиНДС = ЗНАЧЕНИЕ(Перечисление.ВидыДеятельностиНДС.Необлагаемая)
			ТОГДА Затраты.НДСРеглОстаток
		КОГДА ТИПЗНАЧЕНИЯ(Затраты.ДокументВыпуска) = ТИП(Документ.ВыпускПродукции) 
			  ИЛИ ТИПЗНАЧЕНИЯ(Затраты.ДокументВыпуска) = ТИП(Документ.ПоступлениеОтПереработчика) 
			  ИЛИ ТИПЗНАЧЕНИЯ(Затраты.ДокументВыпуска) = ТИП(Документ.ОтчетПереработчика) 
			ТОГДА ВЫБОР
				КОГДА 
					(Затраты.АналитикаУчетаПартий.НалоговоеНазначение.ВидДеятельностиНДС = ЗНАЧЕНИЕ(Перечисление.ВидыДеятельностиНДС.Облагаемая) И Затраты.ДокументВыпуска.НалоговоеНазначение.ВидДеятельностиНДС = ЗНАЧЕНИЕ(Перечисление.ВидыДеятельностиНДС.Необлагаемая))
					ИЛИ (Затраты.АналитикаУчетаПартий.НалоговоеНазначение.ВидДеятельностиНДС = ЗНАЧЕНИЕ(Перечисление.ВидыДеятельностиНДС.ПропорциональноОблагаемая) И Затраты.ДокументВыпуска.НалоговоеНазначение = ЗНАЧЕНИЕ(Справочник.НалоговыеНазначенияАктивовИЗатрат.НДС_НеоблагаемаяНеХозДеятельность))
				ТОГДА Затраты.НДСРеглОстаток
				ИНАЧЕ 0 
