		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	НАЧАЛОПЕРИОДА(НачисленныйУдержанныйНДФЛ.Период, МЕСЯЦ) КАК Период,
	НАЧАЛОПЕРИОДА(НачисленныйУдержанныйНДФЛ.НалоговыйПериод, МЕСЯЦ) КАК НалоговыйПериод,
	НачисленныйУдержанныйНДФЛ.ГоловнаяОрганизация,
	НачисленныйУдержанныйНДФЛ.Организация,
	НачисленныйУдержанныйНДФЛ.ФизическоеЛицо,
	СУММА(НачисленныйУдержанныйНДФЛ.Налог) КАК СуммаНалога,
	НачисленныйУдержанныйНДФЛ.КодДохода
ПОМЕСТИТЬ ВТНачисленныйУдержанныйНДФЛ
ИЗ                     
	РегистрНакопления.РасчетыНалогоплательщиковСБюджетомПоНДФЛ КАК НачисленныйУдержанныйНДФЛ
ГДЕ
	НачисленныйУдержанныйНДФЛ.НалоговыйПериод МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(НачисленныйУдержанныйНДФЛ.Период, МЕСЯЦ),
	НачисленныйУдержанныйНДФЛ.НалоговыйПериод,
	НачисленныйУдержанныйНДФЛ.ГоловнаяОрганизация,
	НачисленныйУдержанныйНДФЛ.Организация,
	НачисленныйУдержанныйНДФЛ.ФизическоеЛицо,
	НачисленныйУдержанныйНДФЛ.КодДохода
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	НАЧАЛОПЕРИОДА(СведенияОДоходах.Период, МЕСЯЦ) КАК Период,
	НАЧАЛОПЕРИОДА(СведенияОДоходах.НалоговыйПериод, МЕСЯЦ) КАК НалоговыйПериод,
	СведенияОДоходах.ГоловнаяОрганизация,
	СведенияОДоходах.Организация,
	СведенияОДоходах.ФизическоеЛицо,
	0 КАК СуммаНалога,
	СУММА(СведенияОДоходах.СуммаДохода) КАК СуммаДохода,
	СведенияОДоходах.КодДохода,
	СведенияОДоходах.Подразделение
ПОМЕСТИТЬ ВТСведенияНДФЛ
ИЗ
	РегистрНакопления.СведенияОДоходахНДФЛ КАК СведенияОДоходах
ГДЕ
	СведенияОДоходах.НалоговыйПериод МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода

СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(СведенияОДоходах.Период, МЕСЯЦ),
	НАЧАЛОПЕРИОДА(СведенияОДоходах.НалоговыйПериод, МЕСЯЦ),
	СведенияОДоходах.ГоловнаяОрганизация,
	СведенияОДоходах.Организация,
	СведенияОДоходах.ФизическоеЛицо,
	СведенияОДоходах.КодДохода,
	СведенияОДоходах.Подразделение
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ИсчисленныйНДФЛ.Период КАК Период,
	ИсчисленныйНДФЛ.Организация КАК Организация,
	ИсчисленныйНДФЛ.ФизическоеЛицо КАК ФизическоеЛицо,
	ИсчисленныйНДФЛ.КодДохода КАК КодДохода,
	ИсчисленныйНДФЛ.НалоговыйПериод КАК НалоговыйПериод,
	СУММА(ИсчисленныйНДФЛ.СуммаЛьготы) КАК СуммаЛьготы
ПОМЕСТИТЬ ВТИсчисленныйНДФЛ
ИЗ
	РегистрНакопления.ИсчисленныйНДФЛ КАК ИсчисленныйНДФЛ
ГДЕ
	ИсчисленныйНДФЛ.НалоговыйПериод МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода

СГРУППИРОВАТЬ ПО
	ИсчисленныйНДФЛ.Период,
	ИсчисленныйНДФЛ.НалоговыйПериод,
	ИсчисленныйНДФЛ.Организация,
	ИсчисленныйНДФЛ.ФизическоеЛицо,
	ИсчисленныйНДФЛ.КодДохода
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	СведенияОДоходах.Период КАК Период,
	СведенияОДоходах.НалоговыйПериод КАК НалоговыйПериод,
	СведенияОДоходах.ГоловнаяОрганизация,
	СведенияОДоходах.Организация,
	СведенияОДоходах.ФизическоеЛицо,
	СУММА(НачисленныйУдержанныйНДФЛ.СуммаНалога) КАК СуммаНалога,
	СУММА(СведенияОДоходах.СуммаДохода) КАК Доход,
	СУММА(ИсчисленныйНДФЛ.СуммаЛьготы) КАК СуммаЛьготы,
	СведенияОДоходах.КодДохода,
	СведенияОДоходах.Подразделение
ПОМЕСТИТЬ ВТПолныеСведенияНДФЛ
ИЗ
	ВТСведенияНДФЛ КАК СведенияОДоходах
		ПОЛНОЕ СОЕДИНЕНИЕ ВТНачисленныйУдержанныйНДФЛ КАК НачисленныйУдержанныйНДФЛ
		ПО СведенияОДоходах.Организация = НачисленныйУдержанныйНДФЛ.Организация
			И СведенияОДоходах.НалоговыйПериод = НачисленныйУдержанныйНДФЛ.НалоговыйПериод
			И СведенияОДоходах.ФизическоеЛицо = НачисленныйУдержанныйНДФЛ.ФизическоеЛицо
			И СведенияОДоходах.КодДохода = НачисленныйУдержанныйНДФЛ.КодДохода
		ЛЕВОЕ СОЕДИНЕНИЕ ВТИсчисленныйНДФЛ КАК ИсчисленныйНДФЛ
		ПО СведенияОДоходах.Организация = ИсчисленныйНДФЛ.Организация
			И СведенияОДоходах.НалоговыйПериод = ИсчисленныйНДФЛ.НалоговыйПериод
			И СведенияОДоходах.ФизическоеЛицо = ИсчисленныйНДФЛ.ФизическоеЛицо
			И СведенияОДоходах.КодДохода = ИсчисленныйНДФЛ.КодДохода
ГДЕ
	СведенияОДоходах.НалоговыйПериод МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода

СГРУППИРОВАТЬ ПО
	СведенияОДоходах.Период,
	СведенияОДоходах.НалоговыйПериод,
	СведенияОДоходах.ГоловнаяОрганизация,
	СведенияОДоходах.Организация,
	СведенияОДоходах.ФизическоеЛицо,
	СведенияОДоходах.КодДохода,
	СведенияОДоходах.Подразделение
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПолныеСведенияНДФЛ.Период КАК Период,
	ПолныеСведенияНДФЛ.НалоговыйПериод КАК НалоговыйПериод,
	ПолныеСведенияНДФЛ.ГоловнаяОрганизация КАК ГоловнаяОрганизация,
	ПолныеСведенияНДФЛ.Организация КАК Организация,
	ПолныеСведенияНДФЛ.ФизическоеЛицо КАК ФизическоеЛицо,
	СУММА(ПолныеСведенияНДФЛ.Доход) КАК Доход,
	СУММА(ПолныеСведенияНДФЛ.СуммаНалога) КАК СуммаНалога,
	СУММА(ПолныеСведенияНДФЛ.СуммаЛьготы) КАК СуммаЛьготы,
	ПолныеСведенияНДФЛ.Подразделение КАК Подразделение,
	ПолныеСведенияНДФЛ.КодДохода КАК КодДохода
{ВЫБРАТЬ
	Период,
	НалоговыйПериод,
	ГоловнаяОрганизация.*,
	Организация.*,
	ФизическоеЛицо.*,
	Подразделение.*,
	КодДохода,
	Доход,
	СуммаНалога,
	СуммаЛьготы}
ИЗ
	ВТПолныеСведенияНДФЛ КАК ПолныеСведенияНДФЛ
{ГДЕ
	ПолныеСведенияНДФЛ.Период,
	ПолныеСведенияНДФЛ.НалоговыйПериод,
	ПолныеСведенияНДФЛ.ГоловнаяОрганизация.*,
	ПолныеСведенияНДФЛ.Организация.*,
	ПолныеСведенияНДФЛ.ФизическоеЛицо.*,
	ПолныеСведенияНДФЛ.Подразделение.*,
	ПолныеСведенияНДФЛ.КодДохода,
	ПолныеСведенияНДФЛ.Доход,
	ПолныеСведенияНДФЛ.СуммаНалога,
	ПолныеСведенияНДФЛ.СуммаЛьготы}

СГРУППИРОВАТЬ ПО
	ПолныеСведенияНДФЛ.Период,
	ПолныеСведенияНДФЛ.НалоговыйПериод,
	ПолныеСведенияНДФЛ.ГоловнаяОрганизация,
	ПолныеСведенияНДФЛ.Организация,
	ПолныеСведенияНДФЛ.ФизическоеЛицо,
	ПолныеСведенияНДФЛ.Подразделение,
	ПолныеСведенияНДФЛ.КодДохода</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
	<calculatedField>
		<dataPath>ПериодФормированияОтчета</dataPath>
		<expression>Выбор
    Когда НачалоПериода(&amp;Период.ДатаНачала, "МЕСЯЦ") = НачалоПериода(&amp;Период.ДатаОкончания, "МЕСЯЦ")
        Тогда Формат(&amp;Период.ДатаНачала, "ДФ='ММММ гггг'")
    Иначе Формат(&amp;Период.ДатаНачала, "ДФ='ММММ гггг'") + " - " + Формат(&amp;Период.ДатаОкончания, "ДФ='ММММ гггг'")
Конец</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Период формирования отчета</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>uk</v8:lang>
				<v8:content>Період формування звіту</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<totalField>
		<dataPath>Доход</dataPath>
		<expression>Сумма(Доход)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаНалога</dataPath>
		<expression>Сумма(СуммаНалога)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаЛьготы</dataPath>
		<expression>Сумма(СуммаЛьготы)</expression>
	</totalField>
	<parameter>
		<name>НачалоПериода</name>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Начало периода</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>uk</v8:lang>
				<v8:content>Початок періоду</v8:content>
			</v8:item>
		</title>
		<valueType>
			<v8:Type>xs:dateTime</v8:Type>
			<v8:DateQualifiers>
				<v8:DateFractions>DateTime</v8:DateFractions>
			</v8:DateQualifiers>
		</valueType>
		<value xsi:type="xs:dateTime">0001-01-01T00:00:00</value>
		<useRestriction>true</useRestriction>
		<expression>&amp;Период.ДатаНачала</expression>
