Функция ВариантыНастроек() Экспорт
	
	Результат = Новый Массив;
	
	Вариант = Новый Структура;
	Вариант.Вставить("Имя", "РеестрДоговоровЛизинга");
	Вариант.Вставить("Представление", НСтр("ru='Реестр договоров лизинга';uk='Реєстр договорів лізингу'"));
	Результат.Добавить(Вариант);
	
	Вариант = Новый Структура;
	Вариант.Вставить("Имя", "РасчетыПоДоговоруЛизинга");
	Вариант.Вставить("Представление", НСтр("ru='Расчеты по договору лизинга';uk='Розрахунки за договором лізингу'"));
	Результат.Добавить(Вариант);
	
	Возврат Результат;
	
