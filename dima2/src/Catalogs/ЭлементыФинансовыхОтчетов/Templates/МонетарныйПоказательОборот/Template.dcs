		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗЛИЧНЫЕ РАЗРЕШЕННЫЕ
	&amp;ЭлементОтчета КАК Показатель,
	&amp;ДатаНачала КАК ДатаНачала,
	&amp;ДатаОкончания КАК ДатаОкончания,
	//&amp;ВидСубконтоОборота КАК ВидСубконто,
	//ТаблицаРегистра.Субконто1,
	//ТаблицаРегистра.Организация,
	//ТаблицаРегистра.Подразделение,
	ЕСТЬNULL(ТаблицаРегистра.СуммаОборот, 0) * ВЫБОР
		КОГДА &amp;ОбратныйЗнак = ИСТИНА
			ТОГДА -1
		ИНАЧЕ 1
	КОНЕЦ КАК Значение,
	&amp;КодСтроки КАК КодСтроки,
	&amp;Примечание КАК Примечание
ИЗ
	РегистрБухгалтерии.Международный.Обороты(&amp;ДатаНачала {(&amp;ДатаНачала)}, &amp;ДатаОкончания {(&amp;ДатаОкончания)}, Авто, {(Счет).*}, {(&amp;ВидСубконтоОборота)}, {(Организация).*, (Подразделение).*, (Валюта).*, (Субконто1).*, (Субконто2).*, (Субконто3).*, (ПодразделениеКор).*, (ВалютаКор).*, (КорСубконто1).*, (КорСубконто2).*, (КорСубконто3).*}, {(КорСчет).*}, ) КАК ТаблицаРегистра</query>
	</dataSet>
