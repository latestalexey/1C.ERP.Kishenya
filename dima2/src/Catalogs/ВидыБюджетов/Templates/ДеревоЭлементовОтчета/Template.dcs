		<field xsi:type="DataSetFieldField">
			<dataPath>Значение_СтатьяБюджетов</dataPath>
			<field>Значение_СтатьяБюджетов</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Значение статья бюджетов</v8:content>
				</v8:item>
