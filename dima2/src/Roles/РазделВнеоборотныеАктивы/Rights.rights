									И Значения.ЗначениеДоступа = ГруппыЗначений.ГруппаЗначенийДоступа
							ГДЕ
								ЕСТЬNULL(Значения.ЗначениеРазрешено, ЗначенияПоУмолчанию.ВсеРазрешены)
