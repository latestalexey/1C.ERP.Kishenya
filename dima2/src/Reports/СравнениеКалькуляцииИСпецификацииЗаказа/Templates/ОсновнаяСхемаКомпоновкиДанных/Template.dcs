			<field xsi:type="DataSetFieldField">
				<dataPath>ЦенаСпецификация</dataPath>
				<field>ЦенаСпецификация</field>
				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>ru</v8:lang>
						<v8:content>Цена спецификация</v8:content>
					</v8:item>
					<v8:item>
						<v8:lang>uk</v8:lang>
