		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	СУММА(ОстаткиНачало.СуммаОстаток) КАК СуммаОбщийНачальныйОстаток,
	СУММА(ОстаткиНачало.СуммаПредставленияОстаток) КАК СуммаПредставленияОбщийНачальныйОстаток,
	СУММА(ОстаткиНачало.ВалютнаяСуммаОстаток) КАК ВалютнаяСуммаОбщийНачальныйОстаток,
	"Общие итоги" КАК ОбщиеИтоги,
	ОстаткиНачало.Счет
ПОМЕСТИТЬ ОстаткиНаНачало
{ВЫБРАТЬ
	СуммаОбщийНачальныйОстаток КАК СуммаОбщийНачальныйОстаток,
	СуммаПредставленияОбщийНачальныйОстаток КАК СуммаПредставленияОбщийНачальныйОстаток,
	ВалютнаяСуммаОбщийНачальныйОстаток КАК ВалютнаяСуммаОбщийНачальныйОстаток}
ИЗ
	РегистрБухгалтерии.Международный.Остатки({(&amp;НачалоПериода)}, {(Счет).* КАК Счет}, {(&amp;ВидСубконто)}, {(Субконто1).* КАК Субконто1, (Субконто2).* КАК Субконто2, (Субконто3).* КАК Субконто3, (Подразделение).* КАК Подразделение, (Организация).* КАК Организация, (Валюта).* КАК Валюта}) КАК ОстаткиНачало
{ГДЕ
	ОстаткиНачало.Субконто1.*,
	ОстаткиНачало.Субконто2.*,
	ОстаткиНачало.Субконто3.*}

СГРУППИРОВАТЬ ПО
	ОстаткиНачало.Счет
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	"Общие итоги" КАК ОбщиеИтоги,
	СУММА(ОстаткиКонец.СуммаОстаток) КАК СуммаОбщийКонечныйОстаток,
	СУММА(ОстаткиКонец.СуммаПредставленияОстаток) КАК СуммаПредставленияОбщийКонечныйОстаток,
	СУММА(ОстаткиКонец.ВалютнаяСуммаОстаток) КАК ВалютнаяСуммаОбщийКонечныйОстаток,
	ОстаткиКонец.Счет
ПОМЕСТИТЬ ОстаткиНаКонец
{ВЫБРАТЬ
	СуммаОбщийКонечныйОстаток КАК СуммаОбщийКонечныйОстаток,
	СуммаПредставленияОбщийКонечныйОстаток КАК СуммаПредставленияОбщийКонечныйОстаток,
	ВалютнаяСуммаОбщийКонечныйОстаток КАК ВалютнаяСуммаОбщийКонечныйОстаток}
ИЗ
	РегистрБухгалтерии.Международный.Остатки({(&amp;КонецПериода)}, {(Счет).* КАК Счет}, {(&amp;ВидСубконто)}, {(Субконто1).* КАК Субконто1, (Субконто2).* КАК Субконто2, (Субконто3).* КАК Субконто3, (Подразделение).* КАК Подразделение, (Организация).* КАК Организация, (Валюта).* КАК Валюта}) КАК ОстаткиКонец
{ГДЕ
	ОстаткиКонец.Субконто1.*,
	ОстаткиКонец.Субконто2.*,
	ОстаткиКонец.Субконто3.*}

СГРУППИРОВАТЬ ПО
	ОстаткиКонец.Счет
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ИтогиОбщие.ОбщиеИтоги КАК ОбщиеИтоги,
	СУММА(ИтогиОбщие.СуммаОбщийНачальныйОстаток) КАК СуммаОбщийНачальныйОстаток,
	СУММА(ИтогиОбщие.СуммаОбщийКонечныйОстаток) КАК СуммаОбщийКонечныйОстаток,
	СУММА(ИтогиОбщие.СуммаПредставленияОбщийНачальныйОстаток) КАК СуммаПредставленияОбщийНачальныйОстаток,
	СУММА(ИтогиОбщие.СуммаПредставленияОбщийКонечныйОстаток) КАК СуммаПредставленияОбщийКонечныйОстаток,
	СУММА(0) КАК ВалютнаяСуммаОбщийНачальныйОстаток,
	СУММА(0) КАК ВалютнаяСуммаОбщийКонечныйОстаток
ПОМЕСТИТЬ ТаблицаИтоги
{ВЫБРАТЬ
	ОбщиеИтоги,
	СуммаОбщийНачальныйОстаток,
	СуммаОбщийКонечныйОстаток,
	ВалютнаяСуммаОбщийНачальныйОстаток,
	ВалютнаяСуммаОбщийКонечныйОстаток,
	СуммаПредставленияОбщийНачальныйОстаток,
	СуммаПредставленияОбщийКонечныйОстаток}
ИЗ
	(ВЫБРАТЬ
		Остатки.СуммаОбщийНачальныйОстаток КАК СуммаОбщийНачальныйОстаток,
		Остатки.СуммаОбщийКонечныйОстаток КАК СуммаОбщийКонечныйОстаток,
		Остатки.СуммаПредставленияОбщийНачальныйОстаток КАК СуммаПредставленияОбщийНачальныйОстаток,
		Остатки.СуммаПредставленияОбщийКонечныйОстаток КАК СуммаПредставленияОбщийКонечныйОстаток,
		Остатки.ВалютнаяСуммаОбщийНачальныйОстаток КАК ВалютнаяСуммаОбщийНачальныйОстаток,
		Остатки.ВалютнаяСуммаОбщийКонечныйОстаток КАК ВалютнаяСуммаОбщийКонечныйОстаток,
		Остатки.ОбщиеИтоги КАК ОбщиеИтоги
	{ВЫБРАТЬ
		СуммаОбщийНачальныйОстаток,
		СуммаОбщийКонечныйОстаток,
		ВалютнаяСуммаОбщийНачальныйОстаток,
		ВалютнаяСуммаОбщийКонечныйОстаток,
		СуммаПредставленияОбщийНачальныйОстаток,
		СуммаПредставленияОбщийКонечныйОстаток}
	ИЗ
		(ВЫБРАТЬ
			ЕСТЬNULL(ОстаткиНаНачало.СуммаОбщийНачальныйОстаток, 0) КАК СуммаОбщийНачальныйОстаток,
			ЕСТЬNULL(ОстаткиНаКонец.СуммаОбщийКонечныйОстаток, 0) КАК СуммаОбщийКонечныйОстаток,
			ЕСТЬNULL(ОстаткиНаНачало.ВалютнаяСуммаОбщийНачальныйОстаток, 0) КАК ВалютнаяСуммаОбщийНачальныйОстаток,
			ЕСТЬNULL(ОстаткиНаКонец.ВалютнаяСуммаОбщийКонечныйОстаток, 0) КАК ВалютнаяСуммаОбщийКонечныйОстаток,
			ЕСТЬNULL(ОстаткиНаНачало.СуммаПредставленияОбщийНачальныйОстаток, 0) КАК СуммаПредставленияОбщийНачальныйОстаток,
			ЕСТЬNULL(ОстаткиНаКонец.СуммаПредставленияОбщийКонечныйОстаток, 0) КАК СуммаПредставленияОбщийКонечныйОстаток,
			"Общие итоги" КАК ОбщиеИтоги
		{ВЫБРАТЬ
			СуммаОбщийНачальныйОстаток КАК СуммаОбщийНачальныйОстаток,
